{
  "expo": {
    "name": "PokemonApp",
    "slug": "pokemon-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/pokemon-logo.png",
    "scheme": "assdev15app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/pokemon-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/pokemon-logo.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "src/app"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "root": "src/app"
      },
      "eas": {
        "projectId": "25d4b305-62ac-4fea-aa61-7640626db910"
      }
    }
  }
}
